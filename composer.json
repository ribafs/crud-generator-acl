{
    "name": "ribafs/crud-generator-acl",
    "license": "MIT",
    "description": "Laravel CRUD Generator with ACL support",
    "keywords": [
        "laravel",
        "crud",
        "crud generator",
        "laravel crud generator",
        "api generator"
    ],
    "authors": [
        {
            "name": "Sohel Amin",
            "email": "sohelamincse@gmail.com",
            "homepage": "http://www.Ribafs.com"
        },
        {
            "name": "Ribamar FS",
            "email": "ribafs@gmail.com",
            "homepage": "https://ribafs.github.io"
        }
    ],
    "require": {
        "php": ">=5.6.4|>=7.0",
        "illuminate/support": "^5.3|^6.0|^7.0|^8.0",
        "laravelcollective/html": "^5.3|^6.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^5.7|^8.0|^9.0",
        "orchestra/testbench": "^3.3|^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "Ribafs\\CrudGeneratorAcl\\": "src/"
        },
        "classmap": [
            "tests"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ribafs\\CrudGeneratorAcl\\CrudGeneratorServiceProvider"
            ]
        }
    }
}
